@model WorkOrderTBL
@{
    var tahvil = ViewData["TahvilInfo"] as TahvilFormsViewModel;
}




<div class="container mt-5">
    <div class="row">
        <div class="col-lg-10  mx-auto">
            <div class="card shadow p-1 mb-5 rounded trackCSS mt-1">
                <h5 class="text-center mt-3 ">  درخواست شما ارسال گردید، کد پیگیری شما به شرح ذیل می باشد </h5>
                <br />
                <h1 class="text-center border border-danger w-25 mx-auto" >@Model.WONo</h1>
                <br/>
                <h5 class="ml-3">نام متقاضی :<span>@tahvil.DeliverCode</span> </h5>
                <br />
                <h5 class="ml-3">شماره اموال :<span>@tahvil.AmvalNo</span> </h5>
                <br />
                <h5 class="ml-3">نوع اموال :<span>@tahvil.PartName,@tahvil.PartModelName</span> </h5>
                <br />
                <h5 class="ml-3">شرح درخواست :<span>@Model.NeedDescription</span> </h5>
                <br/>
            </div>

        </div>

    </div>
</div>

