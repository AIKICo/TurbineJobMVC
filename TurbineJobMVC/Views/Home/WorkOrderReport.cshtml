@model IList<WorkOrderDailyReportViewModel>

<div class="row ltr">
    @if (Model != null)
    {
        @if (Model.Count > 0)
        {
            @foreach (var form in Model)
            {
                <div class="timeline">
                    <div class="containerTL rightTL">
                        <div class="contentTL">
                            <h2>@form.ReportDate</h2>
                            <p>@form.ReportComment</p>
                        </div>
                    </div>
                </div>
            }
        }
    }
</div>
